package bg.softuni.fundamentals.AssociativeArrays;

import java.util.*;
import java.util.stream.Collectors;

public class Uprajnenie {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        Map <Double,Integer> numbers = new TreeMap<>();

        String[] numbersAsStrings = scanner.nextLine().split(" ");
        for (int i = 0; i < numbersAsStrings.length; i++) {
            double number = Double.parseDouble(numbersAsStrings[i]);

            Integer ocurence = numbers.get(number);
            if (ocurence == null){
                ocurence = 0;

            }
            numbers.put(number, ocurence +1);

        }


        for (Map.Entry<Double, Integer> entry : numbers.entrySet()) {

            System.out.printf("%.0f -> %d%n",entry.getKey(),entry.getValue());
        }


    }
}
